{
  "version": 2,
  "builds": [
      {
          "src": "src/main.ts",
          "use": "@vercel/node"
      }
  ],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "src/main.ts",
          "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS"]
      }
  ],
  "headers": [
    {
      "source": "/",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}